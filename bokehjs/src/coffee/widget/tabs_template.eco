<ul class="nav nav-tabs">
  <% for tab, i in @tabs: %>
    <li class="<%= @active(i) %>">
      <a href="#tab-<%= tab.get('id') %>" data-toggle="tab"><%= tab.get('title') %></a>
    </li>
  <% end %>
</ul>
<div class="tab-content">
  <% for tab, i in @tabs: %>
    <div class="tab-pane <%= @active(i) %>" id="tab-<%= tab.get('id') %>"></div>
  <% end %>
</div>
